{
  "cfgVersion": 1,
  "dbFile": "data/vdt.db",
  "txtTemplate": "Endorsed candidates:\n$listings\nThis is not legal advice - for legal advice, consult with your attorney, etc.",
  "listingTemplate": "$candidate - $office\n",
  "csv_mappings": {
    "voters": {
      "csvFilePath": "data/voters.csv",
      "delimiter": ",",
      "columns": {
        "voter_id": {
          "csv": "voter_id",
          "type": "INTEGER"
        },
        "first_name": {
          "csv": "name_first",
          "type": "TEXT"
        },
        "last_name": {
          "csv": "name_last",
          "type": "TEXT"
        },
        "birth_date": {
          "csv": "birth_date",
          "type": "TEXT"
        },
        "precinct": {
          "csv": "precinct",
          "type": "TEXT"
        },
        "phone_1": {
          "csv": "phone_1",
          "type": "TEXT"
        },
        "phone_2": {
          "csv": "phone_2",
          "type": "TEXT"
        }
      }
    },
    "districts": {
      "csvFilePath": "data/districts.csv",
      "delimiter": "\t",
      "columns": {
        "precinct": {
          "csv": "precinct",
          "type": "TEXT"
        },
        "district": {
          "csv": "name_1",
          "type": "TEXT"
        }
      }
    },
    "candidates": {
      "csvFilePath": "data/candidates.csv",
      "delimiter": ",",
      "columns": {
        "name": {
          "csv": "Name",
          "type": "TEXT"
        },
        "office": {
          "csv": "Office",
          "type": "TEXT"
        },
        "district": {
          "csv": "District",
          "type": "TEXT"
        },
        "triggers_precinct": {
          "csv": "Triggers Precinct",
          "type": "INTEGER"
        },
        "unopposed": {
          "csv": "Unopposed",
          "type": "INTEGER"
        },
        "weight": {
          "csv": "Sort Weight",
          "type": "INTEGER"
        }
      }
    }
  }
}
